#include<bits/stdc++.h>
using namespace std;

int findNthFibonacci(int n) {
        if( n== 1 || n == 2){
                cout << 1;
        }
        int current;
        int prev1 = 1;
        int prev2 = 1;
        if(n>2) {
                for (int i = 3; i <= n; i++) {
                  current = prev1 + prev2;
                  prev1 = prev2;
                  prev2 = current;
                }
                cout << current;
        }
}

int main()
{
        int n;
        cin >> n;
        findNthFibonacci(n);
}
